            var num2 = prompt(" Enter the Num2 = ");
            var num3 = prompt(" Enter the Num3 = ");

        if(num1 > num2 && num1 > num3 )
            alert("large number is = " + num1 );
  else if ( num2 > num1 && num2 > num3)
            alert("large number is = " + num2 );
        else
            alert("larger number is = " + num3 );    
        </script>
    </body>
