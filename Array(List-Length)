            var names = ["Sadrul"," Tanim"," Chowdhury"," liza"," Saif"];
            document.write("Name list is : " + names + "<br>");
            document.write("Name list length is : " + names.length + "<br>");
            names.push(" Jorina");
            document.write("Name list is : " + names + "<br>");
            document.write("Name list length is : " + names.length);
            
        </script>
        
    </body>
